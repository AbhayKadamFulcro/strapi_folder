{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "DisplayName": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "aliases": {
      "type": "component",
      "repeatable": true,
      "component": "basic-fields.url-aliases"
    },
    "EnableAMP": {
      "type": "boolean"
    },
    "Title": {
      "type": "string"
    },
    "ShortText": {
      "type": "text"
    },
    "Icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
