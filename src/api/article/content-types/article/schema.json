        "files",
        "videos",
        "audios"
      ]
    },
    "ThumbnailDesktop": {
      "type": "component",
      "repeatable": false,
      "component": "basic-fields.thumbnails"
    },
    "ThumbnailMobile": {
      "type": "component",
      "repeatable": false,
      "component": "basic-fields.thumbnails"
    },
    "CreatedDate": {
      "type": "date"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "basic-fields.seo"
    },
    "PageContent": {
      "type": "component",
      "repeatable": false,
      "component": "basic-fields.page-content"
    },
    "layout": {
      "type": "enumeration",
      "enum": [
        "articledetail_v2"
      ]
    },
    "ArticleSchema": {
      "type": "richtext"
    },
    "ExcludeFromSitemap": {
      "type": "boolean"
    },
    "PageType": {
      "type": "enumeration",
      "enum": [
        "tw-page-blog-article"
      ]
    },
    "OCEID": {
      "type": "string"
    },
    "UpdatedDate": {
      "type": "date"
    }
  }
